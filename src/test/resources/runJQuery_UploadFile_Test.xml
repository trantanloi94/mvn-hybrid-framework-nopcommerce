<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="JQuery Upload File">
    <!--    Apply listeners for all testcases-->
    <listeners>
        <listener class-name="actions.commons.MethodListener"></listener>
    </listeners>
    <parameter name="url" value="https://blueimp.github.io/jQuery-File-Upload/"></parameter>
    <test name="Run on Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="testcases.com.jquery.uploadfile.Level_11_UploadFile"></class>
        </classes>
    </test>


</suite>

